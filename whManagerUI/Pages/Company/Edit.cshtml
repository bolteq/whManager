@page
@model whManagerUI.Pages.Company.EditModel
@{
    ViewData["Title"] = "Edit";
}

<h2>Edytowanie firmy @Model.Company.Name</h2>

<form method="post" class="form-inline">
    <div class="form-group">
        <input asp-for="Company.Name" class="form-control" />
        <input asp-for="Company.Address" class="form-control" />
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
</form>

